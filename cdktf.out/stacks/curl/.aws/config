[profile curl]
region=us-west-1

[profile curl-net-sso]
sso_start_url=
sso_region=us-west-2
sso_role_name=Administrator
sso_account_id=101142583332
output=json

[profile curl-net]
credential_process=aws-vault exec curl-net-sso --json
region=us-west-1

[profile curl-net-org]
source_profile=curl
role_arn=arn:aws:iam::101142583332:role/OrganizationAccountAccessRole
region=us-west-1

[profile curl-net-adm]
credential_process=aws-vault exec curl-net-org --json
region=us-west-1

